@model ContactInformation
<footer class="footer_part mt-5">
    <div class="container">
        <div class="row justify-content-between">
          
            <div class="col-sm-6 col-lg-2">
                <div class="single_footer_part">
                    <h4>Şirkət</h4>
                    <ul class="list-unstyled">
                        <li><a asp-controller="About" asp-action="Index">Haqqımızda</a></li>
                        
                        <li><a href="">FAQ</a></li>
                        <li><a asp-controller="Contact" asp-action="Index">Əlaqə</a></li>
                    </ul>
                </div>
            </div>
            <div class="col-sm-6 col-lg-3">
                <div class="single_footer_part">
                    <h4>Address</h4>
                    <ul class="list-unstyled">
                        <li><a href="#">@Model.Address</a></li>
                        <li><a href="#">@Model.Email</a></li>
                        <li><span>@Model.Phone</span></li>
                    </ul>
                </div>
            </div>

        </div>
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="copyright_text">
                    <p> Bütün müəllif hüquqları qorunur</p>
                </div>

            </div>
        </div>
    </div>

</footer>
