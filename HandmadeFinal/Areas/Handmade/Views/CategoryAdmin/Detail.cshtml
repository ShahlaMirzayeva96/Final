@model Category
@{
    ViewData["Title"] = "Detail";
}

<div class="container-fluid" style="margin-top:90px;">
    <div class="row">
        <div class="col-12">
            
            <p>@Model.CategoryName</p>
            <img src="~/img/@Model.CategoryImage" width="300px" />
        </div>
    </div>
    <div class="row">

        <div class="col-12">
            <a asp-action="Index">Go Back</a>
        </div>
    </div>
</div>

