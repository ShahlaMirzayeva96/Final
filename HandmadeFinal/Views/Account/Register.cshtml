@model RegisterSellerView
@{
    ViewData["Title"] = "Qeydiyyat";
}

<section class="breadcrumb breadcrumb_bg">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="breadcrumb_iner">
                    <div class="breadcrumb_iner_item">
                        <p>Ana Səhifə/Qeydiyyat</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


<section class="login_part section_padding">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 col-md-6">
                <div class="login_part_text text-center">
                    <div class="login_part_text_iner">
                        <h2>New to our Shop?</h2>
                        <p>
                            There are advances being made in science and technology
                            everyday, and a good example of this is the
                        </p>
                        <div class="btn-group dropright">
                            <button type="button" class="btn_3 btn btn-secondary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                              
                            </button>
                           
                        </div>
                    </div>
                </div>
            </div>


            <div id="sellerRegister-section" class="col-lg-6 col-md-6 mt-5">
                <div class="login_part_form">
                    <div class="login_part_form_iner">
                        <h3>
                            Qeydiyyat
                        </h3>

                        <form class="row contact_form" method="post">
                            <div asp-validation-summary="ModelOnly"></div>
                            <div class="col-md-12 form-group p_star">
                                <input autocomplete="off" asp-for="Email" type="text" class="form-control" id="email" name="email" value=""
                                       placeholder="Email">
                                <span asp-validation-for="Email" class="text-danger"></span>
                            </div>
                            <div class="col-md-12 form-group p_star">
                                <input autocomplete="off" asp-for="UserName" type="text" class="form-control" id="name" name="UserName" value=""
                                       placeholder="Username">
                                <span asp-validation-for="UserName" class="text-danger"></span>
                            </div>
                            <div class="col-md-12 form-group p_star">
                                <input autocomplete="off" asp-for="Name" type="text" class="form-control" id="name" name="name" value=""
                                       placeholder="Ad">
                                <span asp-validation-for="Name" class="text-danger"></span>
                            </div>
                            <div class="col-md-12 form-group p_star">
                                <input autocomplete="off" asp-for="Surname" type="text" class="form-control" id="name" name="Surname" value=""
                                       placeholder="Soyad">
                                <span asp-validation-for="Surname" class="text-danger"></span>
                            </div>


                            <div class="col-md-12 form-group p_star">
                                <input autocomplete="off" asp-for="Phone" type="text" class="form-control" id="phone" name="phone" value=""
                                       placeholder="Telefon">
                                <span asp-validation-for="Phone" class="text-danger"></span>
                            </div>

                            <div class="col-md-12 form-group p_star">
                                <input asp-for="Password" type="password" class="form-control" id="password" name="password" value=""
                                       placeholder="Password">
                                <span asp-validation-for="Password" class="text-danger"></span>
                            </div>
                            <div class="col-md-12 form-group p_star">
                                <input asp-for="ConfirmPassword" type="password" class="form-control" id="password" name="ConfirmPassword" value=""
                                       placeholder="Confirm Password">
                                <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                            </div>
                            <div class="col-md-12 form-group">
                               
                                <input type="submit" value="Qeydiyyatdan Keç" class="btn_3" />
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@section Scripts{
    <partial name="_ValidationScriptsPartial" />
}

